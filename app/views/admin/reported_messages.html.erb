<ul>
  <% @reported_messages.each do |message| %>
    <li>
      <% if message.user %>
        <%= link_to message.user.email, admin_user_path(message.user) %>
      <% else %>
        System message
      <% end %>
      <%= message.text %>
      <%= message.reported_at.to_s :short %>
      <% user = User.find message.reported_by_user_id %>
      <%= link_to "reported_by #{user.email}", admin_user_path(user.id) %>
      <%= link_to 'chat', chat_path(message.chat) %>
    </li>
  <% end %>
</ul>
