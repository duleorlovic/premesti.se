<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to sign_in_as_path(user_id: user.id) do %>
        <%= user.email %>
        <% if user.admin? %>
          <span class='badge badge-primary'>admin</span>
        <% end %>
      <% end %>
      <% user.moves.each do |move| %>
        <%= move.group_age_and_locations %>
        <% if move.chats.present? %>
          <span class='badge badge-warning'>chat</span>
        <% end %>
      <% end %>
    </li>
  <% end %>
</ul>
